{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "public": "dist/public",
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "api/[...].js"
    }
  ]
}
